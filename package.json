    "@radix-ui/react-progress": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "graphql": "^16.8.1",
    "lucide-react": "^0.476.0",
    "next": "15.1.5",
    "payload": "latest",
    "postcss": "^8.5.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
